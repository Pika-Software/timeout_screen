"UnlitGeneric"
{
    "nomip"     "1"
    "nolod"     "1"
    "ignorez"   "1"

    "$vertexcolor" "1"
    "$vertexalpha" "1"
	"$alphatest"   "1"

    "$basetexture"          "pikasoft/timeout_screen/cats"
    "$basetexturetransform" "center .5 .5 scale 1 1 rotate 0 translate 0 0"
    "$translucent"          "1"

    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedTextureVar"            "$basetexture"
            "animatedTextureFrameNumVar"    "$frame"
            "animatedTextureFrameRate"      "4"
        }
    }
}
